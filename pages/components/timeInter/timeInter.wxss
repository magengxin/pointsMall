
/* 计时器部分 */

.time-inter, .round-circle {
  position: absolute;
  width: 56px;
  height: 56px;
  line-height: 56px;
  text-align: center;
  color: #ffebc8;
  left: 44%;
  top: 25%;
  border: 1px solid #ffebc8;
  border-radius: 50%;
}
.out-circle {
  font-size: 14px;
  font-weight: 600;
}
.round-circle {
  width: 56px;
  height: 56px;
  position: absolute;
  left: 44%;
  /* transform: translateX(-100%); */
}

.small-circle {
  width: 6px;
  height: 6px;
  position: absolute;
  top: -3px;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 50%;
  background-color: #ffebc8;
}

.small-circle image {
  width: 10px;
  height: 10px;
  position: absolute;
  left: 50%;
  top: 50%;
}
