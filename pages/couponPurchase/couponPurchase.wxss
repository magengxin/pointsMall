.coupon-purchase{
  width: 100%;
  height: 100%;
  background-color: #f2f2f2;
}
.coupon-purchase .couponmsg-bgc{
  width: 96%;
  /* height: auto; */
  margin:5% auto 0;
  position: relative;
  background:linear-gradient(90deg,rgba(76,76,76,1) 0%,rgba(48,48,48,1) 100%);
  border-radius:12px;
  padding-top:18px 
}
.coupon-title{
  width: 45%;
  margin: auto;
  height: 40px;
  top: -20px
}
.coupon-title-write{
  top: -10px;
  color: #fff;
  font-size:13px;
  font-family:PingFangSC-Medium;
  font-weight:500;
}
.list-coupon{
  width: 96%;
  height: 78px;
  margin:7px auto 0;
  position: relative
}
.list-coupon-bg{
  width: 100%;
  height: 100%;
}
.list-coupon-logo{
  width: 15.07%;
  height: 52px;
  left: 3.77%
}
.list-coupon-name{
  position: absolute;
  top: 12px;
  width: 58%;
  left: 22.03%;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.subheading{
  position: absolute;
  top: 30px;
  left: 22.03%;
  width: 58%;
  font-size:11px;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(179,179,179,1);
}
.list-coupon-value{
  position: absolute;
  left: 22.03%;
  top: 53px;
  font-size:16px;
  font-family:DIN-Bold;
  font-weight:bold;
  color:rgba(188,138,87,1);
}
.coupon-num{
  display: block;
  width: 11.6%; 
  height: 15px;
  text-align: center;
  line-height: 15px;
  background:linear-gradient(to right,#F1C69B,#C58F59);
  border-radius: 15px;
  position: absolute;
  padding: 0 4px;
  top: 56px;
  left: 62.2%;
  font-size:10px;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.exchange{
  width: 15.94%;
  height: 55px;
  border: 2px solid #fff;
  font-size:18px;
  font-family:PingFangSC-Light;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 55px;
  border-radius: 50%;
  position: absolute;
  top: 11.5px;
  left: 81%;
}
.foot-notmessage{
  width: 100%;
  text-align: center;
  font-size: 12px;
  color: #fff;
  margin-top: 10px
}